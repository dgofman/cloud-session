{
  "name": "cloud-session",
  "version": "0.0.1",
  "description": "Session replication library",
  "main": "index.js",
  "scripts": {
    "lint": "jshint . --config .jshintrc",
    "test": "mocha tests/index.js --reporter spec --check-leaks",
